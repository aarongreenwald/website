{
  "name": "website",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm run build; node index.js",
    "prebuild": "mkdir slides/revealjs/; cp -r ./node_modules/reveal.js/css ./node_modules/reveal.js/js ./node_modules/reveal.js/lib ./node_modules/reveal.js/plugin ./slides/revealjs",
    "build": "node-sass --recursive --output ./slides/theme --source-map true --source-map-contents ./slides/theme",
    "watch": "nodemon -w slides/theme -w resources -w pages -w blog -w index.js --ext js,ejs,md,scss --ignore node_modules -x 'npm run start' ",
    "test": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarongreenwald/website.git"
  },
  "author": "Aaron Greenwald",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aarongreenwald/website/issues"
  },
  "homepage": "https://github.com/aarongreenwald/website#readme",
  "dependencies": {
    "compression": "^1.6.2",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "express-partials": "^0.3.0",
    "ip2country": "^1.0.0",
    "moment": "^2.17.1",
    "reveal.js": "^3.4.1",
    "showdown": "^1.5.4",
    "ua-parser-js": "^0.7.12"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.12.2",
    "eslint-config-wix": "^1.1.3",
    "node-sass": "^4.5.1",
    "nodemon": "^1.11.0"
  }
}
